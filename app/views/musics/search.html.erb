<center><h4><p class="bg-danger">Найдено <%= @searching.total %> ветвей обсуждений</p></h4></center>
<div class="row">
  <% @musics.each do |music| %>
    <div class="col-sm-6 col-md-3">
      <div class="thumbnail">
        <%= link_to (image_tag music.image.url(:medium), class: 'image'), music %>
        <strong><%= music.title + " - " + music.artist %></strong>
      </div>
    </div>
  <% end %>
</div>
